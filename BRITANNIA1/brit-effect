                $('.cover_for_border').fadeOut();
                $('#divMobileFooter').fadeIn();
                var scrollLeng = $(document).height() - 600;
                //   alert(scrollLeng + "..." + $(document).height());
                setTimeout($("html, body").animate({ scrollTop: scrollLeng }, 1000), 500);
            }
            else {
                $('#productsFooter').fadeIn();
                $('.cover_for_border').fadeIn();
                $('#divMobileFooter').fadeOut();
                setTimeout($("html, body").animate({ scrollTop: $(document).height() }, 1000), 500);
            }

        });

