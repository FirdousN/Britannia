        //sharing Function
        function fb_share(url, w, h) {
            //console.log(url);
            if (url.length > 0) {
                var encodedUrl = encodeURIComponent(url);
                var link = 'https://www.facebook.com/sharer.php?u=' + encodedUrl;
                //window.open(link, '', 'width=600,height=400,resizable=yes,scrollbars=yes');

                var left = (screen.width / 2) - (w / 2);
                var top = (screen.height / 2) - (h / 2);
                window.open(link, 'FacebookShare', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
            }
            return false;
        }

        function tweet_share(url, text, w, h) {
            //console.log(url);
            if (url.length > 0) {
                var encodedUrl = encodeURIComponent(url);
                //text = text.replace('&', 'and');
                //console.log(text);
                var encodeText = encodeURIComponent(text); //$('<div/>').text(text).html();
                var link = 'https://twitter.com/intent/tweet?text=' + encodeText + '&url=' + encodedUrl;
                //window.open(link, '', 'width=600,height=400,resizable=yes,scrollbars=yes');

                var left = (screen.width / 2) - (w / 2);
                var top = (screen.height / 2) - (h / 2);
                window.open(link, 'TwitterShare', 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
            }
            return false;
        }

        //Get number of FB Likes

        //function to get no of likes
        function getSharesCount(url_share) {
            var original_count = 123;
            var tweet_count = 0;
            var fb_share = 0;
            $.getJSON('http://graph.facebook.com/' + url_share, function (fbdata) {
                fb_share = fbdata.shares;
                $.getJSON('http://cdn.api.twitter.com/1/urls/count.json?url=' + url_share + '&callback=?', function (twitdata) {
                    tweet_count = twitdata.count;
                    original_count = fb_share + tweet_count;
                    if (original_count < 10) {
                        original_count = "0" + original_count;
                    }
                    if (original_count > 0) {
                        $('#total_share').html(original_count);
                    }
                });
            });



        }


