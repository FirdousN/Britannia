	function develop_blue_block(){
		var logo_position = jQuery('.header-logo-block').offset().left ;
		var logo_block_width = jQuery('.header-logo-block').outerWidth(true) / 2;
		var left_block_width = logo_position + logo_block_width ;	
		jQuery('.left-spacer-logo').width(left_block_width);
	}
	jQuery(window).on('load resize', function() {
		develop_blue_block()
	});
	
