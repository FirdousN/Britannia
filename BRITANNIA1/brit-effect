        /* Header Scroll */
        if (!detectIE()) {
            var position = $(window).scrollTop();
            $(window).scroll(function () {
                var scroll = $(window).scrollTop();
                if (scroll < 0) {
                    scroll = 0;
                }
                // console.log("S: " + scroll + "----P: " + position);
                if (scroll > position) {
                    // scrolling downwards
                    //  console.log("H");
                    $('.heading').stop().animate({ "margin-top": "-100px" }, 300);
                } else {
                    //  console.log("Sh 22");
                    $(".heading").stop().animate({ "margin-top": "0px" }, 300);
                }
                $(".func").hide();
                position = scroll;
            });
        }

    });
