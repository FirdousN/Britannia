                getNutritionDetails(temp)
                var flavourDesc = '';
                flavourDesc = $(this).attr('data-dairy');
               
                if (flavourDesc) {
                    //alert('hi')
                    $("#proddet").empty();
                    $("#proddet").html(flavourDesc);
                    $('.para1 p').hide().fadeIn();
                    //   alert(flavourDesc);
                }
                var content = $("#" + temp).find("span").attr("data-id1");
                var img = $("#" + temp).find("img").attr("data-id");
                var permalink = $(this).attr("data-id");
                var headtext = $("#" + temp).find(".like").text();
                videoID = $("#" + temp).find(".like").attr("data-vid");
                var videoImg = $("#" + temp).find(".like").attr("data-img");
                var imgAlt = $("#" + temp).find("img").attr("alt");
                //console.log(headtext+"---------"+videoID + "----------" + videoImg);

                if (videoID) {
                    //  alert(videoID);                  

                    if (VideoBool) {

                        $('.video').hide().html('<img src="' + videoImg + '" class="cvideo video_image" /><a href="#" id="video_clk"><img src="' + siteURL + 'images/play2.png" class="play" /><img src="' + siteURL + 'images/play3.png" class="play2" /></a>').fadeIn();
                        VideoBool = false;
                    }

                    else {
                        $('.video_image').hide().attr("src", videoImg).fadeIn()
                    }
                }
                else {
                    //alert(videoID);
                    $('.video').html('');
                    VideoBool = true;
                }

                if (permalink == 'deuce') {
                    $('#video_clk').hide();
                    $('.cvideo').css('border', 'none');
                    //$('.buy-nw-btn').show();
                    $('#condition').show();
                    $('#headDeuce').show();
                }
                else {
                    $('#video_clk').show();
                    $('.cvideo').css('border', '3px solid #dadada');
                    //$('.buy-nw-btn').hide();
                    $('#condition').hide();
                    $('#headDeuce').hide();
                }

                $('#temp1').hide().html(headtext).fadeIn();
                $('#cphContent_prodImage').hide().attr("src", img).fadeIn();
                $('#cphContent_prodImage').attr("alt", imgAlt);
                $('#temp2').hide().html(content).fadeIn();
                //$('#temp2').text(content);  
                //if (temp == '87') {
                //    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/mag-ad2-4-.jpg").fadeIn();
                //}

                if (temp == '18' || temp == '19') {
                    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/process-Pure-Magic.jpg").fadeIn();
                }
				else if (temp == '89') {
                    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/dark-chocolate-left.jpg").fadeIn();
                }
                else if (temp == '123') {
                    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/white-chocolate-left.jpg").fadeIn();
                }
                
				
				
				
				//else if (temp == '99' || temp == '100') {
                //    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/Nutrichoice-Oats.jpg").fadeIn();
                //}
                //else if (temp == '14'  || temp == '15' || temp == '16'|| temp == '17'|| temp == '58'|| temp == '59'|| temp == '60'|| temp == '88'){
                //    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/process-NC-consumption.jpg").fadeIn();
                //}
                //else if(temp=='101'){
                //    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/essentials.jpg").fadeIn();
                //    $('<noscript class="noscri"><div style="display: inline;"><img height="1" width="1" style="border-style: none;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/876500510/?guid=ON&amp;script=0" /></div></noscript>').appendTo('body');
                //}
                //else if(temp=='112'){
                //    $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/Biscotti-Choconut.jpg").fadeIn();
                // }
                // else if(temp=='111'){
                //     $('#cphContent_bigImage').hide().attr("src", "http://britannia.co.in/images/products/Biscotti-Fruit-n-nut.jpg").fadeIn();
                // }
                
				
				$(".description").mCustomScrollbar("destroy");
                $(".description").mCustomScrollbar({
                    theme: "britannia"
                });

                var url = $(location).attr('href');
                url = url.substring(0, url.lastIndexOf('/'));

                var final_url = url + "/" + permalink;
                if (!$.browser.msie) {        //console.log(pathtophp+hashlist[currentFrame]);                                
                    window.history.replaceState("Product Change", "Product Change", final_url);
                    //$("html, body").animate({ scrollTop: 200 }, "slow");
                }
                $('html, body').animate({
                    scrollTop: $(".goodday").offset().top
                }, "slow");

            })

            //Video Player
            videoID = 'AaYsysScjA8'

            if (!videoID) {
                $('.video').hide();
                VideoBool = true;
            }


            $(document).on('click', '#video_clk', function (e) {
                e.preventDefault();
                VideoBool = true;
                var video_template = ' <iframe src="//www.youtube.com/embed/' + videoID + '?rel=0&vq=hd1080&autoplay=1&modestbranding=1;autohide=1&amp;showinfo=0&amp;controls=1" width="' + $('.video_image').width() + '" height="' + $(".video").height() + '" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>';
                $(this).parent().html(video_template);
            });

            //Slider Initialize Responsive
            var pageWidth = $(window).width();



            if (pageWidth < 600) {
                $('#slideul .dummy').remove();
                $('#slideul').slick({
                    infinite: true,
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    prevArrow: '<img src="http://britannia.co.in/images/left_swipe_red.png" class="red_left" />',
                    nextArrow: '<img src="http://britannia.co.in/images/right_swipe_red.png" class="red_right_products" />',
                    appendArrows: $('.otherRanges'),
                    mobileFirst: true
                    //dots: true,
                    //appendDots: "#customPage"
                });
            }
            else if (pageWidth < 1050) {
                $('#slideul .dummy').remove();
                $('#slideul').slick({
                    infinite: true,
                    slidesToShow: 3,
                    slidesToScroll: 3,
                    dots: true,
                    appendDots: "#customPage",
                    prevArrow: '<img src="http://britannia.co.in/images/left_swipe_red.png" class="red_left_desktop" />',
                    nextArrow: '<img src="http://britannia.co.in/images/right_swipe_red.png" class="red_right_desktop" />'
                });
            }

            else {
                $('#slideul').slick({
                    infinite: true,
                    slidesToShow: 4,
                    slidesToScroll: 4,
                    dots: true,
                    appendDots: "#customPage",
                    prevArrow: '<img src="http://britannia.co.in/images/left_swipe_red.png" class="red_left_desktop" />',
                    nextArrow: '<img src="http://britannia.co.in/images/right_swipe_red.png" class="red_right_desktop" />'
                });
            }

            $(window).resize(function () {
                pageWidth = $(window).width();

                RESIZEE(pageWidth);
            });

            function RESIZEE(pageWidth) {

                if ($(window).width() != pageWidth) {
                    $('#slideul').slick('unslick');
                    if (pageWidth < 600) {
                        $('#slideul .dummy').remove();
                        $('#slideul').slick({
                            infinite: true,
                            slidesToShow: 1,
                            slidesToScroll: 1
                            //dots: true,
                            //appendDots: "#customPage"
                        });
                    }
                    else if (pageWidth < 1050) {
                        $('#slideul .dummy').remove();
                        $('#slideul').slick({
                            infinite: true,
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            dots: true,
                            appendDots: "#customPage"
                        });
                    }

                    else {
                        $('#slideul').slick({
                            infinite: true,
                            slidesToShow: 4,
                            slidesToScroll: 4,
                            dots: true,
                            appendDots: "#customPage"
                        });
                    }

                }
            }

            $('#chart').on("click", function (e) {
                e.preventDefault();
                //getNutritionDetails($(this).attr("data-id"))
                $('.blurr').fadeIn();
                $('.popupNutri').fadeIn();
            })


            $(".blurr").on("click", closeTabs);
            $("#closePopup").on("click", closeTabs);
            function closeTabs() {
                $('.popupNutri').hide();
                $('.blurr').hide();
            }

            function getNutritionDetails(id) {
                $("#modal-scrollbar").mCustomScrollbar("destroy");
                $.ajax({
                    url: "http://britannia.co.in/ajax/getNutritionDetails.aspx",
                    type: "POST",
                    data: {
                        id: id
                    },
                    dataType: "html",
                }).success(function (response) {
                    if (response != "0" && response != null) {
                        $('#modal-scrollbar').empty();
                        $('#modal-scrollbar').append(response);
                        $('#modal-scrollbar').mCustomScrollbar({
                            theme: "britannia"
                        });
                        $('#chartDisplay').css('display','block');
                    }
                    else{
                        $('#chartDisplay').css('display','none');
                    }
                });
            }

            $(".blurr").on("click", closeTabs1);
            $(".closeData").on("click", closeTabs1);
            function closeTabs1() {
                $('#popup').hide();
                $('.popElement').hide();
                $('.blurr').hide();
            }
        });
    </script>
  
       


